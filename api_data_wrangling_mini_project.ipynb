   "outputs": [],
   "source": [
    "#7. What was the median trading volume during this year.\n",
    "\n",
    "def Median(l):\n",
    "    mid_index = len(l)//2\n",
    "    l.sort()\n",
    "    if len(l) % 2 ==0:\n",
    "        return (l[mid_index] + l[mid_index-1])/2\n",
    "    else:\n",
    "        return l[mid_index]"
   ]
